{
  "name": "mentor-match",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "migrate:partnerships": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/migrate-partnerships.ts",
    "migrate:under-review": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/migrate-under-review-status.ts",
    "cleanup:check": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/cleanup-orphaned-refs.ts",
    "cleanup:fix": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/cleanup-orphaned-refs.ts --fix",
    "test": "NODE_ENV=test playwright test",
    "test:ui": "NODE_ENV=test playwright test --ui",
    "test:debug": "NODE_ENV=test playwright test --debug",
    "test:headed": "NODE_ENV=test playwright test --headed",
    "test:report": "playwright show-report",
    "test:verbose": "NODE_ENV=test PLAYWRIGHT_VERBOSE=true playwright test"
  },
  "dependencies": {
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "next": "14.2.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.6.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.22",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "firebase-tools": "^15.1.0",
    "node-fetch": "^2.7.0",
    "playwright": "^1.57.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "overrides": {
    "glob": "^10.0.0"
  }
}
