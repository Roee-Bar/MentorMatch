{
  "name": "mentor-match",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "migrate:partnerships": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/migrate-partnerships.ts",
    "migrate:under-review": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/migrate-under-review-status.ts",
    "cleanup:check": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/cleanup-orphaned-refs.ts",
    "cleanup:fix": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/cleanup-orphaned-refs.ts --fix",
    "test:setup-partnerships": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/setup-test-partnerships.ts",
    "test:remove-partnerships": "npx ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/remove-test-partnerships.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:fast": "playwright test --grep '@fast'",
    "test:e2e:critical": "playwright test --grep '@critical'",
    "test:e2e:smoke": "playwright test --grep '@smoke'",
    "test:e2e:regression": "playwright test --grep '@regression'",
    "test:e2e:network": "playwright test --grep '@network'",
    "test:e2e:api": "playwright test --grep '@api'",
    "test:e2e:ui": "playwright test --grep '@ui'",
    "test:e2e:student": "playwright test --grep '@student'",
    "test:e2e:supervisor": "playwright test --grep '@supervisor'",
    "test:e2e:admin": "playwright test --grep '@admin'",
    "test:e2e:auth": "playwright test --grep '@auth'",
    "emulators:start": "firebase emulators:start",
    "emulators:exec": "firebase emulators:exec --only auth,firestore"
  },
  "dependencies": {
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "next": "14.2.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.6.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.22",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "firebase-tools": "^15.1.0",
    "node-fetch": "^2.7.0",
    "playwright": "^1.57.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "overrides": {
    "glob": "^10.0.0"
  }
}
